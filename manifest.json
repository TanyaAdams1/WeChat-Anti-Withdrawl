{
  "name": "WeChat Anti-Withdrawl",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "若要人不知,除非己莫为",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/jquery-3.3.1.min.js",
        "js/content.js"
      ],
      "matches": [
        "*://wx2.qq.com/*"
      ],
      "run_at": "document_end"
    },
    {
      "all_frames": true,
      "js": [
        "js/redirect.js"
      ],
      "matches": [
        "*://weixin110.qq.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "js/inject.js"
  ],
  "icons": {
    "128": "img/icon.png",
    "64": "img/icon.png",
    "32": "img/icon.png"
  }
}